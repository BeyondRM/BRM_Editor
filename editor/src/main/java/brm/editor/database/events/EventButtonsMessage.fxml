<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?scenebuilder-preview-i18n-resource ../../i18n/Bundle.properties?>
<?scenebuilder-stylesheet ../../appstyle.css?>

<fx:root type="javafx.scene.layout.AnchorPane" styleClass="ButtonPane" xmlns:fx="http://javafx.com/fxml/1" fx:controller="brm.editor.database.events.EventButtons">
  <children>
    <HBox styleClass="HBoxButtonBar" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <Label styleClass="LabelRotate" text="Message-Text" />
        <GridPane styleClass="GridPaneButtonEvents" HBox.hgrow="NEVER">
          <children>
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Show Message Text..." GridPane.columnIndex="0" GridPane.rowIndex="0" />
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Show Scrolling Text..." GridPane.columnIndex="0" GridPane.rowIndex="1" />
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Show Static Text..." GridPane.columnIndex="0" GridPane.rowIndex="2" />
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Show Choice Menu..." GridPane.columnIndex="0" GridPane.rowIndex="3" />
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Number Input..." GridPane.columnIndex="1" GridPane.rowIndex="0" />
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="String Input..." GridPane.columnIndex="1" GridPane.rowIndex="1" />
          </children>
        </GridPane>
        <Separator orientation="VERTICAL" />
      </children>
    </HBox>
  </children>
  <stylesheets>
    <URL value="@../../appstyle.css" />
  </stylesheets>
</fx:root>
