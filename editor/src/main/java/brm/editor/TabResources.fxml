<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?scenebuilder-preview-i18n-resource i18n/Bundle.properties?>
<?scenebuilder-stylesheet appstyle.css?>

<fx:root type="javafx.scene.layout.AnchorPane" xmlns:fx="http://javafx.com/fxml/1" fx:controller="brm.editor.TabResources">
  <children>
    <TabPane styleClass="TabContent2" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <tabs>
        <Tab text="Fonts">
          <content>
            <fx:include source="resource/ResourceFont.fxml" />
          </content>
        </Tab>
        <Tab text="Images">
          <content>
            <fx:include source="resource/ResourceImages.fxml" />
          </content>
        </Tab>
        <Tab text="Music">
          <content>
            <AnchorPane id="Content" />
          </content>
        </Tab>
        <Tab text="Sounds">
          <content>
            <AnchorPane id="Content" />
          </content>
        </Tab>
      </tabs>
    </TabPane>
  </children>
  <stylesheets>
    <URL value="@appstyle.css" />
  </stylesheets>
</fx:root>
