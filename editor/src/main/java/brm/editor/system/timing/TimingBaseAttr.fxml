<fx:root type="javafx.scene.layout.AnchorPane" styleClass="InfoPane" xmlns:fx="http://javafx.com/fxml/1" fx:controller="brm.editor.system.timing.Timing">
  <children>
    <VBox id="VBox" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <Label styleClass="LabelH1" maxWidth="1.7976931348623157E308" text="Base Attributes" />
        <GridPane id="GridPane">
          <children>
            <TextField prefWidth="40.0" GridPane.columnIndex="0" GridPane.rowIndex="0" />
            <TextField prefWidth="40.0" GridPane.columnIndex="0" GridPane.rowIndex="1" />
            <TextField prefWidth="40.0" GridPane.columnIndex="0" GridPane.rowIndex="2" />
            <TextField prefWidth="40.0" GridPane.columnIndex="0" GridPane.rowIndex="3" />
            <Label text="minutes-increment" GridPane.columnIndex="1" GridPane.rowIndex="0" />
            <Label text="ticks per increment" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="minutes per hour" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label text="hours per day" GridPane.columnIndex="1" GridPane.rowIndex="3" />
          </children>
          <columnConstraints>
            <ColumnConstraints hgrow="NEVER" />
            <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" />
          </columnConstraints>
        </GridPane>
      </children>
    </VBox>
  </children>
  <stylesheets>
    <URL value="@../../appstyle.css" />
  </stylesheets>
</fx:root>
